{
  "name": "Extension Task",
  "description": "Build an Extension!",
  "version": "1.0.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["storage","notifications","tabs","activeTab","alarms","scripting"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "/images/one.jpg",
      "32": "/images/two.jpg",
      "48": "/images/three.jpg",
      "128": "/images/four.jpg"
    }
  },
  "icons": {
    "16": "/images/one.jpg",
    "32": "/images/two.jpg",
    "48": "/images/three.jpg",
    "128": "/images/four.jpg"
  },

  "content_scripts": [ 
    {
    "matches": ["<all_urls>"],
    "js": ["Content.js","inject.js"],
    "css": ["inject.css"]
    }
  ],
  
  "web_accessible_resources": [{
    "resources": ["/images/*"],
    "matches": ["http://*/*", "https://*/*"]
  }]
}